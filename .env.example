# Database connection string for PostgreSQL
# On AWS Lightsail, this will be provided by your Lightsail database instance
DATABASE_URL=postgresql://username:password@hostname:5432/database_name

# Secret key for session encryption (generate a random string)
# You can generate one with: openssl rand -base64 32
SESSION_SECRET=your-secret-key-here-change-this

# Password for admin login (choose a secure password)
ADMIN_PASSWORD=your-admin-password-here

# Environment (production or development)
NODE_ENV=production

# Port (Lightsail uses 5000 by default, or you can change it)
PORT=5000
